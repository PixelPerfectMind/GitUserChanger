<Window x:Class="GitUserChanger.Dialogs.GitUserViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GitUserChanger.Dialogs"
        mc:Ignorable="d"
        Title="Git user viewer" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock x:Name="txt_Name" FontWeight="Bold" FontSize="20" />
            <TextBlock x:Name="txt_eMail" />
            <TextBlock x:Name="txt_gpgKey" />
        </StackPanel>
    </Grid>
</Window>
